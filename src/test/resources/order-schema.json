{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "petId": {
      "type": "integer"
    },
    "quantity": {
      "type": "integer"
    },
    "shipDate": {
      "type": "string"
    },
    "status": {
      "type": "string",
      "enum": [
        "placed",
        "approved",
        "delivered"
      ]
    },
    "complete": {
      "type": "boolean"
    }
  },
  "required": [
    "id",
    "petId",
    "quantity",
    "shipDate",
    "status",
    "complete"
  ]
}